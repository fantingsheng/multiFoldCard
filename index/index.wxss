/* pages/ecard/index.wxss */
@font-face {font-family: "iconfont";
  src: url('iconfont.eot?t=1560495067920'); /* IE9 */
  src: url('iconfont.eot?t=1560495067920#iefix') format('embedded-opentype'), /* IE6-IE8 */
  url('data:application/x-font-woff2;charset=utf-8;base64,') format('woff2'),
  url('iconfont.woff?t=1560495067920') format('woff'),
  url('iconfont.ttf?t=1560495067920') format('truetype'), /* chrome, firefox, opera, Safari, Android, iOS 4.2+ */
  url('iconfont.svg?t=1560495067920#iconfont') format('svg'); /* iOS 4.1- */
}

.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-jiantou:before {
  content: "\e612";
}

.icon-tianjia:before {
  content: "\e620";
}

.icon-shuaxin:before {
  content: "\e64c";
}

page {
  height: 100%;
}
.page {
  background-color: rgba(218, 224, 231, 1);
}
._flex {
  display: flex;
}

._flex_item_center {
  align-items: center;
}

._flex_justify_center {
  justify-content: center;
}

._flex_space_between {
  justify-content: space-between;
}

._flex_space_around {
  justify-content: space-around;
}
.header {
  text-align: center;
  flex-direction: column;
  overflow: hidden;
  width: 100%;
  line-height: 1;
  position: fixed;
  top: 0;
  background-color: rgba(218, 224, 231, 1);
  padding-bottom: 40rpx;
}
.header .avatar {
  margin: 30rpx auto 0;
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
  background-color: #fff;
}
.header .avatar image {
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
}
.header ._title {
  margin-top: 28rpx;
  font-size: 46rpx;
  font-weight: 600;
}
.header ._intro {
  margin-top: 14rpx;
  color: rgba(136, 145, 169, 1);
  font-size: 24rpx;
}
._count {
  margin: 60rpx 0 0;
  line-height: 1;
  font-size: 24rpx;
  color: rgba(104, 113, 135, 1);
}
.cardWrap {
  height: 100%;
  padding: 0 20rpx;
  position: relative;
}
.cardWrap.open {
  position: fixed;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  height: 100%;
  background-color: rgba(218, 224, 231, 1);
  overflow-y: hidden;
  z-index: 1111;
}
.cardWrap .card {
  width: 710rpx;
  box-sizing: border-box;
  height: 1004rpx;
  background-color: #fff;
  border-radius: 50rpx;
  position: absolute;
  box-shadow: 0 -4rpx 66rpx 0 rgba(227, 232, 240, 0.55);
  transition: all .6s linear;
}
.w46 {
  padding: 0 46rpx;
}
.cmpinfo {
  padding: 50rpx 0;
  border-bottom: 2rpx solid rgba(202, 202, 202, .5);
  position: relative;
}
.cardWrap .card .logo {
  overflow: hidden;
  height: 80rpx;
  width: 80rpx;
  line-height: 80rpx;
  border-radius: 10rpx;
  margin-right: 20rpx;
  text-align: center;
  background-color: #fff;
  box-shadow: 2rpx 2rpx 30rpx rgba(227, 232, 240, 0.67);
}
.cardWrap .card .logo image { 
  width: 80rpx;
  height: 80rpx; 
}
.cardWrap .card .logo text { 
  font-size: 24rpx;
  line-height: 68rpx;
  font-weight: 500;
  color: rgba(181, 187, 202, 1);
}
.cardWrap .card .name {
  font-size: 34rpx;
  font-weight: 600;
  line-height: 1;
  margin-bottom: 14rpx; 
}
.cardWrap .card .address {
  color: rgba(136, 145, 169, 1);
  font-size: 22rpx;
  line-height: 1;
}
.cardWrap .card .name text { 
  font-size: 20rpx; 
  background-color: rgba(137, 144, 163, .2); 
  color: rgba(137, 144, 163, 1);
  padding: 4rpx 8rpx 4rpx 8rpx;
  border-radius: 4rpx;
  vertical-align: middle;
}
.icon-jiantou {
  color: rgba(155, 164, 187, .6);
  font-size: 20rpx;
}
.cmpinfo ._rgt {
  transform: rotate(-90deg);
  transition: all .3s linear;
}
.current .cmpinfo ._rgt {
  transform: rotate(90deg);
}
.qrcode {
  text-align: center;
  padding: 0 46rpx;
}
.scan-guide {
  text-align: right;
  color: rgba(0, 132, 255, 1);
  font-size: 24rpx;
  margin-top: 28rpx;
  justify-content: flex-end;
  line-height: 1;
}
.scan-guide .qf {
  display: inline-block;
  width: 30rpx;
  height: 30rpx;
  line-height: 30rpx;
  background-color: rgba(0, 132, 255, .1);
  text-align: center;
  margin-right: 10rpx;
  border-radius: 50%;
}
.qrcode .code {
  width: 360rpx;
  height: 360rpx;
  margin: 44rpx auto 22rpx;
  background-color: #eee;
  font-size: 100rpx;
  line-height: 360rpx;
}
.code image {
  width: 360rpx;
  height: 360rpx; 
}
.warn-info {
  font-size: 26rpx;
  color: rgba(136, 145, 169, .6)
}
.warn-info .refreshBtn {
  background-color: rgba(231, 238, 246, 1);
  color: rgba(0, 123, 255, 1);
  font-size: 22rpx;
  padding: 8rpx 14rpx;
  border-radius: 6rpx;
}
.icon-shuaxin {
  font-size: 20rpx;
  margin-right: 6rpx;
}

.scrollView {
  height: 96rpx;
  overflow: hidden;
}
.floor-scroll ._item {
  width: 90rpx;
  height: 96rpx;
  text-align: center;
  display: inline-block;
  color: rgba(26, 42, 75, 1);
  background-color: rgba(248, 249, 251, 1);
  border-radius: 12rpx;
  margin-right: 14rpx;
  transition: all .1s linear;
}
.floor-scroll ._flex {
  flex-direction: column;
  justify-content: center;
  align-items: center;
  height: 96rpx;
}
.floor-scroll ._item:last-child {
  margin-right: 0;
}
.floor-scroll ._item.cur {
  color: #fff;
  background-color: rgba(0, 132, 255, 1);
}
.floor-scroll ._flex .floor {
  font-size: 30rpx;
  line-height: 1;
  font-weight: 600;
}
.floor-scroll ._flex .room {
  font-size: 18rpx;
  line-height: 1;
  margin-top: 10rpx;
}

.chooseFloor {
  margin: 80rpx 0 30rpx;
  padding: 0 0 0 46rpx;
  text-align: center;
}
.chooseFloor .tit {
  line-height: 1;
  font-size: 34rpx;
  color: rgba(26, 42, 75, 1);
}
.add-btn {
  margin: 40rpx 40rpx;
  height: 114rpx;
  line-height: 114rpx;
  text-align: center;
  border: 2rpx dashed rgba(192, 202, 213, 1);
  border-radius: 57rpx;
  color: rgba(148, 158, 180, 1);
  font-size: 34rpx;
}
.add-btn .iconfont {
  margin-right: 16rpx;
}
.page_ft {
  margin-top: 54rpx;
  padding: 0 40rpx;
}

.know-list-title {
  color: rgb(26, 25, 25);
  font-size: 38rpx;
  font-weight: 600;
  margin-bottom: 20rpx;
}

.know-list {
  color: rgba(26, 42, 75, 1);
  font-size: 26rpx;
  line-height: 40rpx;
}

.know-list ._num {
  margin-right: 10rpx;
}

._ft_logo {
  padding: 80rpx 0 40rpx;
  text-align: center;
}

.footer {
  left: 0;
  bottom: 0;
  width: 100%;
  padding: 30rpx 0;
  background: rgba(253, 254, 254, .96);
  border-top: 1rpx solid #eeeeee;
  z-index: 101;
}